class IF extends Statement{
    Expression expression
    List<Statement> statements=[]
}
class While extends Statement{
    Expression expression
    List<Statement> statements=[]
}
class Expression{
    Statement L
    Comparator Comparator
    Statement R
}

enum Comparator{
    G,GE,L,LE,E,NE
}

class Token{
    Type type
    String name
}
enum Type{
    Num,Bool
}

class Statement{

}

class DefineStatement extend Statement{
    Token token
    String value
}



enum Operator{
    Plus,Minus,Mul,Div
}


